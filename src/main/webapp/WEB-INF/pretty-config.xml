<pretty-config xmlns="http://ocpsoft.com/prettyfaces/3.3.3"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://ocpsoft.com/prettyfaces/3.3.3
               http://ocpsoft.com/xml/ns/prettyfaces/ocpsoft-pretty-faces-3.3.3.xsd">

    <!-- anonymous mappings -->
    <url-mapping>
        <pattern value="/pages/home"/>
        <view-id value="/pages/home.xhtml"/>
    </url-mapping>

    <!-- errors -->
    <url-mapping>
        <pattern value="/pages/error/403" />
        <view-id value="/pages/error/403.xhtml" />
    </url-mapping>

    <url-mapping>
        <pattern value="/pages/error/404" />
        <view-id value="/pages/error/404.xhtml" />
    </url-mapping>

    <url-mapping>
        <pattern value="/pages/error/503" />
        <view-id value="/pages/error/503.xhtml" />
    </url-mapping>

    <url-mapping>
        <pattern value="/pages/error/401" />
        <view-id value="/pages/error/401.xhtml" />
    </url-mapping>

</pretty-config>